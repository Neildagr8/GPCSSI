  {
    "manifest_version": 2,
    "name": "SafeSurf",
    "version": "1.0",
    "description": "Extension to detect malicious website",
    "background": {
        "scripts": ["background.js","popup.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "SafeSurf"
    },

    "content_scripts": [
    {
    "matches": [
        "<all_urls>"
    ],
    "js": ["jquery-3.5.1.min.js","content.js"]
    }
   ]
  }
